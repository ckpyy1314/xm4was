        <handler name="XM4WASInHandler" class="com.googlecode.xm4was.websvc.jaxws.ServerInHandler">
            <order phase="CustomHandlerInPhase"/>
        </handler>
    </InFlow>
    <OutFlow>
        <handler name="XM4WASOutHandler" class="com.googlecode.xm4was.websvc.jaxws.ServerOutHandler">
            <order phase="CustomHandlerOutPhase"/>
        </handler>              
